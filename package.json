{
  "name": "@el3um4s/to-try",
  "version": "1.0.0",
  "description": "Wrapper for error handling without try-catch",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "check-updates": "npx npm-check-updates",
    "check-updates:minor": "npx npm-check-updates --target minor",
    "check-updates:patch": "npx npm-check-updates --target patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/el3um4s/to-try.git"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "typescript",
    "npm",
    "try-catch",
    "ts",
    "js",
    "functional",
    "javascript",
    "functional-programming",
    "npm-package"
  ],
  "author": "Samuele C. De Tomasi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/el3um4s/to-try/issues"
  },
  "homepage": "https://github.com/el3um4s/to-try#readme",
  "devDependencies": {
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "ansi-regex": ">=6.0.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.1",
    "jest": "^29.1.1",
    "ts-jest": "^29.0.2",
    "typescript": "^4.8.4"
  }
}
